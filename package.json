{
  "name": "trackerapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "devstart": "babel-node ./app",
    "start": "npm run build && node ./build/app",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel --ignore node_modules -d ./build . -s",
    "clean": "rm -rf ./build && mkdir build",
    "server": "nodemon --exec babel-node ./app"
  },
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "mongoose": "^5.9.17",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "prettier": "2.0.5"
  }
}
